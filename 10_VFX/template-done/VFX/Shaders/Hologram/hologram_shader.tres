// VectorCompose:9
	float n_in9p0 = 0.00000;
	float n_in9p2 = 0.00000;
	vec3 n_out9p0 = vec3(n_in9p0, n_out12p0, n_in9p2);


// VectorOp:8
	vec3 n_out8p0 = n_out4p0 + n_out9p0;


// Output:0
	VERTEX = n_out8p0;


// Input:3
	mat4 n_out3p0 = MODEL_MATRIX;


// TransformVectorMult:5
	vec3 n_out5p0 = (n_out3p0 * vec4(n_out4p0, 1.0)).xyz;


