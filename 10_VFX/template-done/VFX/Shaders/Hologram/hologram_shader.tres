	float n_out13p1 = n_out10p0.y;
	float n_out13p2 = n_out10p0.z;
	float n_out13p3 = n_out10p0.w;


// Mix:12
	vec3 n_out12p0 = mix(vec3(n_out11p0.xyz), vec3(n_out14p0.xyz), vec3(n_out13p3));


// VaryingGetter:5
	vec3 n_out5p0 = var_WorldPos;


// VectorDecompose:3
	float n_out3p0 = n_out5p0.x;
	float n_out3p1 = n_out5p0.y;
	float n_out3p2 = n_out5p0.z;


// FloatParameter:16
	float n_out16p0 = ScanLineSpeed;


// Input:15
	float n_out15p0 = TIME;


// FloatOp:17
	float n_out17p0 = n_out16p0 * n_out15p0;


// FloatOp:18
	float n_out18p0 = n_out3p1 + n_out17p0;


// FloatParameter:6
	float n_out6p0 = ScanLineSize;


// FloatOp:4
	float n_out4p0 = mod(n_out18p0, n_out6p0);


	float n_out8p0;
// Remap:8
	float n_in8p1 = 0.00000;
	float n_in8p3 = 0.00000;
	float n_in8p4 = 1.00000;
	{
		float __input_range = n_out6p0 - n_in8p1;
		float __output_range = n_in8p4 - n_in8p3;
		n_out8p0 = n_in8p3 + __output_range * ((n_out4p0 - n_in8p1) / __input_range);
	}


// FloatOp:9
	float n_in9p1 = 2.00000;
	float n_out9p0 = pow(n_out8p0, n_in9p1);


// FloatParameter:22
	float n_out22p0 = FresnelPower;


// Fresnel:19
	float n_out19p0 = pow(1.0 - clamp(dot(NORMAL, VIEW), 0.0, 1.0), n_out22p0);


// FloatOp:20
	float n_out20p0 = n_out9p0 + n_out19p0;


// Clamp:21
	float n_in21p1 = 0.00000;
	float n_in21p2 = 1.00000;
	float n_out21p0 = clamp(n_out20p0, n_in21p1, n_in21p2);


// Output:0
	ALBEDO = n_out12p0;
	ALPHA = n_out21p0;
	EMISSION = n_out12p0;


}
